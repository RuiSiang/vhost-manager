server  {
    listen         80;
    listen         90;
    listen         100;
    server_name    test1.vhost.cathaybc.com;
    location / {
        autoindex           off;
        proxy_set_header    X-Forwarded-For $remote_addr;
        proxy_set_header    Host $http_host;
        proxy_pass          http://127.0.0.1:$server_port;
    }

}
